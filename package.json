{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "MERN E-Commerce",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "v16.13.0",
    "npm": "8.1.0"
  },
  "scripts": {
    "start": "node ./backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder -i",
    "data:destroy": "node backend/seeder -d",
    "build": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KenanDVLPR/eCommerce.git"
  },
  "author": "Kenan DropiÄ‡",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KenanDVLPR/eCommerce/issues"
  },
  "homepage": "https://github.com/KenanDVLPR/eCommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.5",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.20"
  }
}
